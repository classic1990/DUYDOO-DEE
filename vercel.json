{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/assets/$1"
    },
    {
      "src": "/login.html",
      "dest": "/client/login.html"
    },
    {
      "src": "/admin.html",
      "dest": "/client/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html",
      "check": true
    }
  ]
}